package g.g.d.com.movie.vo;

public class MoveUserGenre {

	private String drama;
	private String war;
	private String action;
	private String family;
	private String adult;
	private String crime;
	private String mystery;
	private String animation;
	private String comedy;
	private String horror;
	private String romance;
	private String SF;
	private String documentary;
	private String thriller;
	private String musical;
	private String opera;
	private String fantasy;
	private String adventure;
	private String historical;
	private String western;
	
	public MoveUserGenre() {}
	
	public MoveUserGenre(String drama, String war, String action, String family, String adult, String crime,
			String mystery, String animation, String comedy, String horror, String romance, String sF,
			String documentary, String thriller, String musical, String opera, String fantasy, String adventure,
			String historical, String western) {
		this.drama = drama;
		this.war = war;
		this.action = action;
		this.family = family;
		this.adult = adult;
		this.crime = crime;
		this.mystery = mystery;
		this.animation = animation;
		this.comedy = comedy;
		this.horror = horror;
		this.romance = romance;
		SF = sF;
		this.documentary = documentary;
		this.thriller = thriller;
		this.musical = musical;
		this.opera = opera;
		this.fantasy = fantasy;
		this.adventure = adventure;
		this.historical = historical;
		this.western = western;
	}

	public String getDrama() {
		return drama;
	}
	public String getWar() {
		return war;
	}
	public String getAction() {
		return action;
	}
	public String getFamily() {
		return family;
	}
	public String getAdult() {
		return adult;
	}
	public String getCrime() {
		return crime;
	}
	public String getMystery() {
		return mystery;
	}
	public String getAnimation() {
		return animation;
	}
	public String getComedy() {
		return comedy;
	}
	public String getHorror() {
		return horror;
	}
	public String getRomance() {
		return romance;
	}
	public String getSF() {
		return SF;
	}
	public String getDocumentary() {
		return documentary;
	}
	public String getThriller() {
		return thriller;
	}
	public String getMusical() {
		return musical;
	}
	public String getOpera() {
		return opera;
	}
	public String getFantasy() {
		return fantasy;
	}
	public String getAdventure() {
		return adventure;
	}
	public String getHistorical() {
		return historical;
	}
	public String getWestern() {
		return western;
	}
	public void setDrama(String drama) {
		this.drama = drama;
	}
	public void setWar(String war) {
		this.war = war;
	}
	public void setAction(String action) {
		this.action = action;
	}
	public void setFamily(String family) {
		this.family = family;
	}
	public void setAdult(String adult) {
		this.adult = adult;
	}
	public void setCrime(String crime) {
		this.crime = crime;
	}
	public void setMystery(String mystery) {
		this.mystery = mystery;
	}
	public void setAnimation(String animation) {
		this.animation = animation;
	}
	public void setComedy(String comedy) {
		this.comedy = comedy;
	}
	public void setHorror(String horror) {
		this.horror = horror;
	}
	public void setRomance(String romance) {
		this.romance = romance;
	}
	public void setSF(String sF) {
		SF = sF;
	}
	public void setDocumentary(String documentary) {
		this.documentary = documentary;
	}
	public void setThriller(String thriller) {
		this.thriller = thriller;
	}
	public void setMusical(String musical) {
		this.musical = musical;
	}
	public void setOpera(String opera) {
		this.opera = opera;
	}
	public void setFantasy(String fantasy) {
		this.fantasy = fantasy;
	}
	public void setAdventure(String adventure) {
		this.adventure = adventure;
	}
	public void setHistorical(String historical) {
		this.historical = historical;
	}
	public void setWestern(String western) {
		this.western = western;
	}

	
}
