package g.g.d.com.login.vo;

public class LoginVO {

	private String lid;
	private String mpw;
	
	public LoginVO() {}

	public LoginVO(String mid, String mpw) {

		this.lid = mid;
		this.mpw = mpw;
	}

	public String getLid() {
		return lid;
	}

	public String getMpw() {
		return mpw;
	}

	public void setLid(String mid) {
		this.lid = mid;
	}

	public void setMpw(String mpw) {
		this.mpw = mpw;
	}
	
	
	
}
