<update id="emotionUpdate" parameterType="emotionVO" >
	UPDATE EMOTION_TEST
	SET ESUBJECT = #{esubject, jdbcType=VARCHAR}
	WHERE ENUM = '0001'
</update>

<select id="emotionSelect" parameterType="emotionVO" resultType="emotionVO">
	SELECT * FROM EMOTION_TEST
</select>
