package g.g.d.com.login.vo;

public class LoginVO {

	private String lid;
	private String lpw;
	
	public LoginVO() {}

	public LoginVO(String mid, String mpw) {

		this.lid = mid;
		this.lpw = mpw;
	}

	public String getLid() {
		return lid;
	}

	public String getLpw() {
		return lpw;
	}

	public void setLid(String mid) {
		this.lid = mid;
	}

	public void setLpw(String mpw) {
		this.lpw = mpw;
	}
	
	
	
}
