<mapper namespace="g.g.d.com.login.dao.LoginDAO">

<select id="loginCompleteService" parameterType="mem" resultMap="mem">
SELECT MID
,MNUM
,MPW
,MNAME
,MGENDER
,MEMAIL
FROM TUMEM
WHERE MID=#{mid} AND
MPW=#{mpw} AND
MDELETEYN='Y'
	</select>
</mapper>