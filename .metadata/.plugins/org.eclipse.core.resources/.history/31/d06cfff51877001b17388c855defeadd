<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="g.g.d.com.common.dao.ChabunDAO">

<select resultType="board" id="getBoardChabun">SELECT NVL(MAX(SUBSTR(A.BNUM, -4)), 0) + 1 BNUM FROM BOARD A </select>
<select resultType="rboard" id="getRboardChabun">SELECT NVL(MAX(SUBSTR(A.RBNUM, -4)), 0) + 1 RBNUM FROM RBOARD A </select>

</mapper>